{
  "name": "doculisp",
  "version": "3.4.12",
  "description": "A scripting language to build markdown documents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "doculisp": "./dist/index.js"
  },
  "scripts": {
    "precompile": "rimraf src/**/*.js test/**/*.js dist/**/* -g",
    "compile": "tsc",
    "build": "tsc",
    "build:check-all": "tsc --project tsconfig.tests.json --noEmit",
    "build:check-clean": "pwsh -c \"tsc --project tsconfig.tests.json --noEmit 2>&1 | Where-Object { $_ -like '*tests/*' -or $_ -like '*src/*' -or $_ -like 'Found * error*' }\"",
    "test": "jest tests --coverage",
    "start": "ts-node ./src/index.ts",
    "build:docs": "npx doculisp documentation/doculisp.dlproj",
    "compile:docs": "npx doculisp documentation/doculisp.dlproj"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Doculisp/DoculispTypeScript.git"
  },
  "keywords": [
    "markdown",
    "scripting",
    "language"
  ],
  "author": "Jason-Kerney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Doculisp/DoculispTypeScript/issues"
  },
  "homepage": "https://github.com/Doculisp/DoculispTypeScript#readme",
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "^20.12.12",
    "all-contributors-cli": "^6.26.1",
    "approvals": "^7.1.2",
    "booklisp": "^2.0.0",
    "jest": "^29.3.1",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "source-map-support": "^0.5.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@slimio/npm-registry": "^0.7.0",
    "@types/figlet": "^1.7.0",
    "commander": "^12.1.0",
    "figlet": "^1.8.0",
    "glob": "^10.4.1"
  }
}
